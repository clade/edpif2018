\PYG{n}{derivee\PYGZus{}theorique} \PYG{o}{=} \PYG{n}{cos}\PYG{p}{(}\PYG{n}{x}\PYG{p}{)}

\PYG{n+nb}{print}\PYG{p}{(}\PYG{l+s+s1}{\PYGZsq{}}\PYG{l+s+s1}{Valeur numerique :}\PYG{l+s+s1}{\PYGZsq{}}\PYG{p}{,}\PYG{n}{derivee\PYGZus{}calculee}\PYG{p}{)}
\PYG{n+nb}{print}\PYG{p}{(}\PYG{l+s+s2}{\PYGZdq{}}\PYG{l+s+s2}{Ecart avec la valeur theorique (formule 1):}\PYG{l+s+s2}{\PYGZdq{}}\PYG{p}{,}  \PYG{n}{derivee\PYGZus{}calculee}\PYG{o}{\PYGZhy{}}\PYG{n}{derivee\PYGZus{}theorique}\PYG{p}{)}

\PYG{n}{derivee\PYGZus{}calculee} \PYG{o}{=} \PYG{p}{(}\PYG{n}{sin}\PYG{p}{(}\PYG{n}{x}\PYG{o}{+}\PYG{n}{epsilon}\PYG{p}{)} \PYG{o}{\PYGZhy{}} \PYG{n}{sin}\PYG{p}{(}\PYG{n}{x}\PYG{o}{\PYGZhy{}}\PYG{n}{epsilon}\PYG{p}{)}\PYG{p}{)}\PYG{o}{/}\PYG{p}{(}\PYG{l+m+mi}{2}\PYG{o}{*}\PYG{n}{epsilon}\PYG{p}{)}
\PYG{n+nb}{print}\PYG{p}{(}\PYG{l+s+s2}{\PYGZdq{}}\PYG{l+s+s2}{Ecart avec la valeur theorique (formule 2):}\PYG{l+s+s2}{\PYGZdq{}}\PYG{p}{,}  \PYG{n}{derivee\PYGZus{}calculee}\PYG{o}{\PYGZhy{}}\PYG{n}{derivee\PYGZus{}theorique}\PYG{p}{)}


\PYG{k}{def} \PYG{n+nf}{derivative}\PYG{p}{(}\PYG{n}{f}\PYG{p}{,} \PYG{n}{epsilon}\PYG{o}{=}\PYG{l+m+mi}{1}\PYG{n}{E}\PYG{o}{\PYGZhy{}}\PYG{l+m+mi}{5}\PYG{p}{)}\PYG{p}{:}
    \PYG{k}{def} \PYG{n+nf}{derivative\PYGZus{}of\PYGZus{}f}\PYG{p}{(}\PYG{n}{x}\PYG{p}{,} \PYG{n}{epsilon}\PYG{o}{=}\PYG{n}{epsilon}\PYG{p}{)}\PYG{p}{:}
